{
  "compilerOptions": {
    "baseUrl": "./src",
    "rootDir": "./",
    "paths": {
      "@/*": ["*"],
      "@shared/*": ["../shared/*"]
    },
    "target": "es5",
      "lib": ["dom", "es2015"],
      "jsx": "react-jsx",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
  },
}
